<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

	<name>stock-billetterie</name>
	<class>fr.paris.lutece.plugins.stock.modules.billetterie.service.BilletteriePlugin
	</class>
	<version>1.0.5</version>
	<description>module.stock.billetterie.module.description</description>
	<documentation/>
	<installation/>
	<changes/>
	<user-guide/>
	<provider>module.stock.billetterie.module.provider</provider>
	<provider-url>http://lutece.paris.fr</provider-url>
	<icon-url>images/admin/skin/plugins/stock/stock.png</icon-url>
	<copyright>Copyright 2001-2010 Mairie de Paris</copyright>
	<core-version-dependency>
		<min-core-version>2.4.1</min-core-version>
		<max-core-version/>
	</core-version-dependency>

	<db-pool-required>1</db-pool-required>

    <applications>
        <application>
            <application-id>billetterie</application-id>
            <application-class>fr.paris.lutece.plugins.stock.modules.billetterie.web.StockBilletterieApp</application-class>
        </application>
        <application>
            <application-id>reservation</application-id>
            <application-class>fr.paris.lutece.plugins.stock.modules.billetterie.web.StockBilletterieReservationApp</application-class>
        </application>
    </applications>
    
	<!-- Administration interface parameters -->
	<admin-features>
		<admin-feature>
			<feature-id>STATISTICS_MANAGEMENT</feature-id>
			<feature-title>
				module.stock.billetterie.adminFeature.statisticsManagement.name
			</feature-title>
			<feature-description>
				module.stock.billetterie.adminFeature.statisticsManagement.description
			</feature-description>
			<feature-level>4</feature-level>
			<feature-url>
				jsp/admin/plugins/stock/modules/billetterie/ManageStatistics.jsp
			</feature-url>
			<feature-group>
				BILLETTERIE
			</feature-group>
			<feature-icon-url>
				images/admin/skin/plugins/stock/modules/billetterie/statistics.png
			</feature-icon-url>
		</admin-feature>
		<admin-feature>
			<feature-id>PARTNERS_MANAGEMENT</feature-id>
			<feature-title>
				module.stock.billetterie.adminFeature.partnersManagement.name
			</feature-title>
			<feature-description>
				module.stock.billetterie.adminFeature.partnersManagement.description
			</feature-description>
			<feature-level>4</feature-level>
			<feature-url>
				jsp/admin/plugins/stock/modules/billetterie/ManagePartners.jsp
			</feature-url>
			<feature-group>
				BILLETTERIE
			</feature-group>
			<feature-icon-url>
				images/admin/skin/plugins/stock/modules/billetterie/partner.png
			</feature-icon-url>
		</admin-feature>
		<admin-feature>
			<feature-id>OFFERS_MANAGEMENT</feature-id>
			<feature-title>
				module.stock.billetterie.adminFeature.offresManagement.name
			</feature-title>
			<feature-description>
				module.stock.billetterie.adminFeature.offresManagement.description
			</feature-description>
			<feature-level>4</feature-level>
			<feature-url>
				jsp/admin/plugins/stock/modules/billetterie/ManageOffers.jsp
			</feature-url>
			<feature-group>
				BILLETTERIE
			</feature-group>
			<feature-icon-url>
				images/admin/skin/actions/seance.png
			</feature-icon-url>
		</admin-feature>
		<admin-feature>
			<feature-id>CATEGORIES_MANAGEMENT</feature-id>
			<feature-title>
				module.stock.billetterie.adminFeature.categoriesManagement.name
			</feature-title>
			<feature-description>
				module.stock.billetterie.adminFeature.categoriesManagement.description
			</feature-description>
			<feature-level>4</feature-level>
			<feature-url>
				jsp/admin/plugins/stock/modules/billetterie/ManageCategories.jsp
			</feature-url>
			<feature-group>
				BILLETTERIE
			</feature-group>
			<feature-icon-url>
				images/admin/skin/actions/category.png
			</feature-icon-url>
		</admin-feature>
		<admin-feature>
			<feature-id>PRODUCTS_MANAGEMENT</feature-id>
			<feature-title>
				module.stock.billetterie.adminFeature.productsManagement.name
			</feature-title>
			<feature-description>
				module.stock.billetterie.adminFeature.productsManagement.description
			</feature-description>
			<feature-level>4</feature-level>
			<feature-url>
				jsp/admin/plugins/stock/modules/billetterie/ManageProducts.jsp
			</feature-url>
			<feature-group>
				BILLETTERIE
			</feature-group>
			<feature-icon-url>
				images/admin/skin/actions/show.png
			</feature-icon-url>
		</admin-feature>
		<admin-feature>
			<feature-id>PURCHASES_MANAGEMENT</feature-id>
			<feature-title>
				module.stock.billetterie.adminFeature.purchasesManagement.name
			</feature-title>
			<feature-description>
				module.stock.billetterie.adminFeature.purchasesManagement.description
			</feature-description>
			<feature-level>4</feature-level>
			<feature-url>
				jsp/admin/plugins/stock/modules/billetterie/ManagePurchase.jsp
			</feature-url>
			<feature-group>
				BILLETTERIE
			</feature-group>
			<feature-icon-url>
				images/admin/skin/plugins/stock/modules/billetterie/ticket.png
			</feature-icon-url>
		</admin-feature>
	</admin-features>
	
	<servlets>
	  <servlet>
	    <servlet-name>BilletterieSolrSearch</servlet-name>
	    <servlet-class>fr.paris.lutece.plugins.stock.modules.billetterie.web.BilletterieSolrSearch</servlet-class>
   		<url-pattern>/servlet/plugins/billetterieSolrSearch</url-pattern>
	  </servlet>
	  <servlet>
	    <servlet-name>PosterImageServlet</servlet-name>
	    <servlet-class>fr.paris.lutece.plugins.stock.modules.billetterie.web.PosterImageServlet</servlet-class>
   		<url-pattern>/servlet/plugins/poster</url-pattern>
	  </servlet>
	</servlets>
	
    <!-- Dashboard components -->
    <dashboard-components>
    	<dashboard-component>
            <dashboard-component-name>billeterieAdminDashboardComponent</dashboard-component-name>
            <dashboard-component-class>fr.paris.lutece.plugins.stock.modules.billetterie.web.BilleterieDashboardComponent</dashboard-component-class>
        	<dashboard-feature-right>STATISTICS_MANAGEMENT</dashboard-feature-right>
        </dashboard-component>
    </dashboard-components>
    
	 <portlets>
		<portlet>
			<portlet-class>fr.paris.lutece.plugins.stock.modules.billetterie.business.portlet.BilletteriePortletHome
			</portlet-class>
			<portlet-type-id>BILLETTERIE_PORTLET</portlet-type-id>
			<portlet-type-name>module.stock.billetterie.portlet.name</portlet-type-name>
		    <portlet-creation-url>plugins/stock/modules/billetterie/CreatePortletBilletterie.jsp
			</portlet-creation-url>
			<portlet-update-url>plugins/stock/modules/billetterie/ModifyPortletBilletterie.jsp</portlet-update-url>
			<portlet-modify-action-url>plugins/stock/modules/billetterie/DoModifyPortletBilletterie.jsp</portlet-modify-action-url>
			<portlet-create-action-url>plugins/stock/modules/billetterie/DoCreatePortletBilletterie.jsp</portlet-create-action-url>
			<portlet-create-script-template>/admin/portlet/script_create_portlet.html</portlet-create-script-template>
            <portlet-modify-script-template>/admin/portlet/script_modify_portlet.html</portlet-modify-script-template>
			<portlet-create-specific-template>/admin/plugins/stock/modules/billetterie/portlet_billetterie.html</portlet-create-specific-template>
			<portlet-modify-specific-template>/admin/plugins/stock/modules/billetterie/portlet_billetterie.html</portlet-modify-specific-template>
			<portlet-create-specific-form-template/>
			<portlet-modify-specific-form-template/>
		</portlet>
	</portlets>
	

</plug-in>